<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="finding_stage_draft" model="qms.finding.stage">
        <field name="name">Draft</field>
        <field name="sequence">0</field>
        <field name="state">draft</field>
    </record>

    <record id="finding_stage_analysis" model="qms.finding.stage">
        <field name="name">Analysis</field>
        <field name="sequence">50</field>
        <field name="state">analysis</field>
    </record>

    <record id="finding_stage_pending" model="qms.finding.stage">
        <field name="name">Action Plan</field>
        <field name="sequence">100</field>
        <field name="state">pending</field>
    </record>

    <record id="finding_stage_open" model="qms.finding.stage">
        <field name="name">In Progress</field>
        <field name="sequence">150</field>
        <field name="state">open</field>
    </record>

    <record id="finding_stage_done" model="qms.finding.stage">
        <field name="name">Closed</field>
        <field name="sequence">200</field>
        <field name="state">done</field>
    </record>

    <record id="finding_stage_cancel" model="qms.finding.stage">
        <field name="name">Cancelled</field>
        <field name="sequence">250</field>
        <field name="state">cancel</field>
    </record>

</odoo>
